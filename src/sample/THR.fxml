<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.THRController">
   <left>
      <VBox spacing="33.0" style="-fx-background-color: grey;" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="10.0" />
         </padding>
         <children>
            <HBox alignment="TOP_RIGHT" spacing="8.0">
               <children>
                  <Label text="Age:" />
                  <TextField fx:id="ageTextField" prefWidth="50.0" />
               </children>
            </HBox>
            <Button fx:id="calculateButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#calculateButtonPressed" text="Calculate" />
         </children>
      </VBox>
   </left>
   <center>
      <VBox alignment="TOP_CENTER" spacing="8.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="resultTextArea" maxWidth="1.7976931348623157E308" prefHeight="90.0" prefWidth="250.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
